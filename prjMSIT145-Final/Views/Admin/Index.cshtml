@model IEnumerable<prjMSIT145_Final.Models.AdminMember>

@{
    ViewData["Title"] = "Index";
    //Layout = "~/Views/Shared/Admin/_AdminLogin.cshtml";
    Layout = "_Layout";
}

<h1>Index</h1>

<h3>@ViewBag.AdminAcc</h3>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Account)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RoleLevel)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Account)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RoleLevel)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Fid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Fid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Fid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
